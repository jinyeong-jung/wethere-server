{
  "name": "wethere-server",
  "version": "1.0.0",
  "description": "Server for the chat service for couples named \"We There\" made with GraphQL, Typescript, NodeJS.",
  "main": "index.js",
  "repository": "https://github.com/jinyeong-jung/wethere-server.git",
  "author": "Jinyeong Jung <jinyeong32@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.4",
    "@types/helmet": "^0.0.43",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.2",
    "@types/twilio": "^0.0.10",
    "babel-runtime": "^6.26.0",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^1.18.11",
    "ts-node": "^8.0.3",
    "tslint-config-prettier": "1.17.0",
    "typescript": "^3.4.3"
  },
  "scripts": {
    "predev": "yarn run types",
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts,graphql",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "graphql-tools": "4.0.3",
    "graphql-yoga": "^1.17.4",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "pg": "^7.9.0",
    "twilio": "3.27.1",
    "typeorm": "0.2.9"
  }
}
